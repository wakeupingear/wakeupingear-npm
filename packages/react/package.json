{
    "name": "@wakeupingear/react",
    "version": "1.0.7",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "style": "dist/index.css",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist && bun build src/index.ts --outdir ./dist",
        "publish": "bun run build && NODE_AUTH_TOKEN=$(cat ../../.env | grep NPM_TOKEN | cut -d '=' -f2) bunx nx release publish --projects=@wakeupingear/react --foreground-scripts false"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.0",
        "@types/react-transition-group": "^4.4.11",
        "typescript": "^4.5.0"
    },
    "dependencies": {
        "@radix-ui/themes": "^3.1.3",
        "clsx": "^2.1.1",
        "react-icons": "^5.3.0",
        "react-spinners": "^0.14.1",
        "react-transition-group": "^4.4.5"
    },
    "description": "Lots of neat universal components to quickly make a site feel good.",
    "author": "Abby Farhat",
    "license": "ISC",
    "repository": {
        "url": "github:wakeupingear/wakeupingear-npm",
        "type": "git",
        "directory": "packages/react"
    }
